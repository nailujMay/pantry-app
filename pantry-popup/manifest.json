{
    "name": "Pantry Popup",
    "description": "Where you can view your pantry assets and save to your collection",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "src/background.ts"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "contextMenus"
      ]
  }
  