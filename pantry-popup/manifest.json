{
    "name": "Muse Popup",
    "description": "Where you can view your pantry assets and save to your collection",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "src/background.ts"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["src/content.tsx"]
        }
    ],
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "contextMenus"
      ]
  }
  